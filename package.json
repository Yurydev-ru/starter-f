{
  "name": "starter-full-project",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --ext .js,.ts,.vue,.html,.css,.md",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "prepare": "husky install",
    "generate-pwa-assets": "pwa-assets-generator"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix --max-warnings 0",
    "*.{html,md}": "eslint --fix --max-warnings 0",
    "*.{css,scss}": "stylelint --fix --max-warnings 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@vite-pwa/nuxt": "^1.0.4",
    "nuxt": "^3.17.5",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/css": "^0.9.0",
    "@eslint/js": "^9.29.0",
    "@html-eslint/eslint-plugin": "^0.42.0",
    "@html-eslint/parser": "^0.42.0",
    "@stylistic/eslint-plugin": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vite-pwa/assets-generator": "^1.0.0",
    "eslint": "^9.29.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^51.2.3",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^17.20.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^10.2.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "sass": "^1.89.2",
    "typescript": "^5.2.x",
    "vue-eslint-parser": "^10.1.4",
    "vue-tsc": "^2.2.10"
  }
}
